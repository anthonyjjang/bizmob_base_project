{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "// 2": "/** Serve **********************************************************************************/",
    "serve-local": "cross-env NODE_ENV=development VITE_APP_PROXY=on vite",
    "serve-local:deploy": "cross-env NODE_ENV=production VITE_APP_PROXY=on vite",
    "serve-sit": "cross-env NODE_ENV=development VITE_APP_PROXY=on vite --mode sit",
    "serve-sit:deploy": "cross-env NODE_ENV=production VITE_APP_PROXY=on vite --mode sit",
    "serve-uat": "cross-env NODE_ENV=development VITE_APP_PROXY=on vite --mode uat",
    "serve-uat:deploy": "cross-env NODE_ENV=production VITE_APP_PROXY=on vite --mode uat",
    "serve-prod": "cross-env NODE_ENV=development VITE_APP_PROXY=on vite --mode prod",
    "serve-prod:deploy": "cross-env NODE_ENV=production VITE_APP_PROXY=on vite --mode prod",
    "// 3": "/** Build **********************************************************************************/",
    "build-sit": "vite build --mode sit",
    "build-uat": "vite build --mode uat",
    "build-prod": "vite build --mode prod",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode sit",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.14",
    "@fullcalendar/daygrid": "^6.1.14",
    "@fullcalendar/interaction": "^6.1.14",
    "@fullcalendar/timegrid": "^6.1.14",
    "@fullcalendar/vue3": "^6.1.14",
    "@mdi/font": "^7.4.47",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@tiptap/vue-3": "^2.4.0",
    "apexcharts": "^3.49.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.1.6",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "remixicon": "^4.3.0",
    "url-safe-base64": "^1.3.0",
    "vee-validate": "^4.13.1",
    "vue": "^3.4.29",
    "vue-draggable-next": "^2.2.1",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3",
    "vue-scrollto": "^2.20.0",
    "vue-tabler-icons": "^2.21.0",
    "vue3-apexcharts": "^1.5.3",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vuetify": "^3.7.1",
    "vuex": "^4.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/dompurify": "^3.0.5",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-tsc": "^2.0.21"
  }
}
